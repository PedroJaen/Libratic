{
  "rules": {
    ".read": true,
    ".write": "auth.uid != null",
    "users": {
      ".indexOn": [
        "libraryID",
        "userLevel"
      ]
    },
    "libraries": {
      ".indexOn": [
        "id",
        "name"
      ]
    },
    "books": {
      "$bookId": {
        ".indexOn": [
          "title"
        ],
        "owned": {
          "$uid": {
            ".indexOn": [
              "id"
            ]
          }
        }
      }
    },
    "movies": {
      "$movieId": {
        ".indexOn": [
          "title"
        ],
        "owned": {
          "$uid": {
            ".indexOn": [
              "id"
            ]
          }
        }
      }
    },
    "tvs": {
      "$tvId": {
        ".indexOn": [
          "name"
        ],
        "owned": {
          "$uid": {
            ".indexOn": [
              "id"
            ]
          }
        }
      }
    },
    "lendings": {}
  }
}